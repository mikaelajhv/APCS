import java.util.ArrayList;

import mjhv.shapes.Circle;
import mjhv.shapes.Line;
import processing.core.PApplet;

public class DrawSurface extends PApplet{
	
	private ArrayList<Line> lines;
	private Line red1, red2;
	
	public DrawSurface()
	{
		lines.add(new Line(1, 200, 499, 150));
		lines.add(new Line(1, 205, 499, 165));
		lines.add(new Line(1, 210, 499, 180));
		lines.add(new Line(1, 215, 499, 195));
		lines.add(new Line(1, 220, 499, 210));
		lines.add(new Line(1, 225, 499, 225));
		lines.add(new Line(1, 230, 499, 240));
		lines.add(new Line(1, 235, 499, 255));
		lines.add(new Line(1, 240, 499, 270));
		lines.add(new Line(1, 245, 499, 285));
		
		runSketch();
	}
	
	
	
	public void draw()
	{
		background(255);
		
		
		for(int i = 0; i < lines.size(); i++)
		{
			Line l = lines.get(i);
			stroke(0);
			//l.draw(this);
		}
		

		
	}
	

}
